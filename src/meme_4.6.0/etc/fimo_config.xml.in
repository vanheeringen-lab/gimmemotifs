<appConfig xmlns="http://nbcr.sdsc.edu/opal/types" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata>
    <usage><![CDATA[fimo [optional arguments] <mfile> <sequence file>]]></usage>
    <info xsd:type="xsd:string">
    <![CDATA[
        fimo [options] <motif file> <sequence file>

           Options:
             --bgfile <background> (default from motif file)
             --max-seq-length <int> (default=1e6)
             --motif <id> (default=all)
             --motif-name <string> (default from motif file)
             --motif-pseudo <float> (default=0.1)
             --output-pthresh <float> (default 1e-6)
             --qvalue
             --gff <filename>
             --verbosity [1|2|3|4] (default 2)
    ]]>
    </info>
  </metadata>
  <binaryLocation>@BINARYLOCATION@</binaryLocation>
  <parallel>false</parallel>
</appConfig>
