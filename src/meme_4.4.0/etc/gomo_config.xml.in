<appConfig xmlns="http://nbcr.sdsc.edu/opal/types" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata>
    <usage><![CDATA[gomo <scoring file> <organism> [optional arguments] ]]></usage>
    <info xsd:type="xsd:string">
    <![CDATA[
        gomo [options] <scoring file> <organism>

	   Options:
	     --motif <id> (default=all)
	     --verbosity [1|2|3|4] (default 2)
	     --go-map [g2s|s2g] mapping of the go-term file: go-term to sequence (g2s) or opposite (default g2s)
	     --o <output dir> name of the directory for output, will not replacing existing dir
	     --oc <output dir> name of the directory for output, will replace existing dir
	     --text FLAG output in text format only
	     --nostatus FLAG prevent output of the progress report to the terminal
    ]]>
    </info>
  </metadata>
  <binaryLocation>@BINARYLOCATION@</binaryLocation>
  <parallel>false</parallel>
</appConfig>
