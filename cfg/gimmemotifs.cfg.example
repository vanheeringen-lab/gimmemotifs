[main]
index_dir = /usr/share/gimmemotifs/genome_index
bg = /usr/share/gimmemotifs/bg
template_dir = /usr/share/gimmemotifs/templates
score_dir = /usr/share/gimmemotifs/score_dists
gene_dir = /usr/share/gimmemotifs/genes
motif_databases = /usr/share/gimmemotifs/motif_databases
seqlogo = /usr/share/gimmemotifs/tools/seqlogo
tools = /usr/share/gimmemotifs/tools

[params]
fraction = 0.2
use_strand = False
abs_max = 1000
analysis = medium
enrichment = 1.5
width = 200
lwidth = 500
genome = hg18
background = gc,random
cluster_threshold = 0.95
scan_cutoff = 0.9
available_tools = MDmodule,MEME,Weeder,GADEM,MotifSampler,trawler,Improbizer,BioProspector,Posmo,ChIPMunk,AMD,Jaspar
tools = MDmodule,MEME,Weeder,MotifSampler,trawler,Improbizer,BioProspector,Posmo,ChIPMunk,AMD,Jaspar
pvalue = 0.001
max_time = None
ncpus = 12
motif_db = gimme.vertebrate.v3.1.pwm
scan_cutoff = 0.9
use_cache = False

[AMD]
bin = /usr/share/gimmemotifs/tools/AMD.bin
dir = /usr/share/gimmemotifs/tools/

[MEME]
bin = /usr/share/gimmemotifs/tools/meme.bin
dir = /usr/share/gimmemotifs/tools/

[MDmodule]
bin = /usr/share/gimmemotifs/tools/MDmodule
dir = /usr/share/gimmemotifs/tools/

[Improbizer]
bin = /usr/local/bin/ameme
dir = /usr/local/bin/

[MotifSampler]
bin = /usr/local/bin/MotifSampler
dir = /usr/local/bin/

[GADEM]
bin = /usr/share/gimmemotifs/tools/gadem
dir = /usr/share/gimmemotifs/tools/

[trawler]
bin = /usr/share/gimmemotifs/tools/trawler/bin/trawler.pl
dir = /usr/share/gimmemotifs/tools/trawler/

[Weeder]
bin = /usr/share/Weeder/weederTFBS.out
dir = /usr/share/Weeder/

[BioProspector]
bin = /usr/share/gimmemotifs/tools/BioProspector
dir = /usr/share/gimmemotifs/tools/

[ChIPMunk]
bin = /usr/share/gimmemotifs/tools/ChIPMunk/ChIPMunk.sh
dir = /usr/share/gimmemotifs/tools/ChIPMunk/
